! do not scroll with output
URxvt*scrollTtyOutput: false

! scroll in relation to buffer (with mouse scroll or Shift+Page Up)
URxvt*scrollWithBuffer: true

! scroll back to the bottom on keypress
URxvt*scrollTtyKeypress: true

URxvt.secondaryScreen: 1
URxvt.secondaryScroll: 0
!URxvt.font: xft:Source Code Pro:size=14
URxvt.font: xft:Hack:size=14
!URxvt.font: xft:Fira Code Retina:size=14
URxvt*font: xft:Terminus:size=12,xft:Inconsolata\ for\ Powerline:size=12

!URxvt.boldFont: xft:Hack bold:size=14
!URxvt.font:     xft:bitstream vera sans mono:size=16:antialias=true
!URxvt.boldFont: xft:bitstream vera sans mono:bold:size=16:antialias=true

URxvt.letterSpace: -1
URxvt.perl-ext-common: default,matcher
URxvt.url-launcher: /usr/bin/xdg-open
URxvt.matcher.button: 1
URxvt.matcher.rend.0: Uline Bold fg5

URxvt.perl-ext-common:  default,resize-font
URxvt.iso14755: false
URxvt.iso14755_52: false

URxvt.iconFile:  /usr/share/icons/breeze-dark/apps/16/utilities-terminal.svg

URxvt.depth: 32
!URxvt.background: [95]#000000


 
URxvt.scrollBar: false
 

URxvt*foreground:   #fcfcfc
URxvt*background:   [90]#232629
URxvt*cursorColor:  #d1b217

! black
URxvt*color0:       #31363b
URxvt*color8:       #4d4d4d

! red
URxvt*color1:       #ed2929
URxvt*color9:       #ed1515

!green
URxvt*color2:       #21d96d
URxvt*color10:      #0bd961

!yellow
URxvt*color3:       #fcc74c
URxvt*color11:      #fde04b

!blue
URxvt*color4:       #3daee9
URxvt*color12:      #3d95e9

!magenta
URxvt*color5:       #9847b5
URxvt*color13:      #a94fc9

!cyan
URxvt*color6:       #1abc9c
URxvt*color14:      #1cdc9a

!white
URxvt*color7:       #eff0f1
URxvt*color15:      #fcfcfc


